#include <iostream>
#include <string>
using namespace std;


  string getLastUniqueElement(string str) {

	  int length = str.length();
	  string result="null";
	  for (size_t i = 0; i < length; ++i) 
	  {
		  for (size_t j = 0; j < length ; ++j)
		  {
			  if (j == i)continue;
			  else if (str[i] == str[j]) 
			  {
				  break;
			  }
			  else if (j == length - 1)
			  {
				  result = str[i];
			  }
			  else if (i == length - 1 && j == length - 2) {
				  result = str[i];
				  return result;
			  }
		  }
	  }
	  return result;
}

 int main(){

	 cout << "Result:" << getLastUniqueElement("abbcbfcad") << endl;
     getchar();
     return 0;
 }
